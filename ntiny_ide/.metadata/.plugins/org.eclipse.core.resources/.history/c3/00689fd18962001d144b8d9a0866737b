void int_disable(void);
void int_enable(void);

extern __attribute__((section (".ISR_I2C_ASM"))) __attribute__((interrupt())) void ISR_I2C_ASM();
extern __attribute__((section (".ISR_UART_ASM"))) __attribute__((interrupt())) void ISR_UART_ASM();
extern __attribute__((section (".ISR_EXT_ASM"))) __attribute__((interrupt())) void ISR_EXT_ASM();
extern __attribute__((section (".ISR_SPI_ASM"))) __attribute__((interrupt())) void ISR_SPI_ASM();
extern __attribute__((section (".ISR_TIMER_ASM"))) __attribute__((interrupt())) void ISR_TIMER_ASM();

